    "paths": {
        "/invoke/js": {
            "post": {
                "description": "Run JavaScript with optional data input",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Invoke"
                ],
                "summary": "Invoke JavaScript",
                "parameters": [
                    {
                        "description": "request body",
                        "name": "body",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/main.jsInvokeReq"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/main.jsInvokeRes"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/main.jsInvokeErrRes"
                        }
                    },
                    "422": {
                        "description": "Unprocessable Entity",
                        "schema": {
                            "$ref": "#/definitions/main.jsInvokeErrRes"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "main.jsInvokeErrRes": {
            "type": "object",
            "required": [
                "error"
            ],
            "properties": {
                "error": {
                    "type": "string"
                },
                "source": {
                    "type": "string"
                },
                "stackTrace": {
                    "type": "string"
                }
            }
        },
        "main.jsInvokeReq": {
            "type": "object",
            "required": [
                "script"
            ],
            "properties": {
                "data": {
                    "type": "string",
                    "example": "test_data"
                },
                "script": {
                    "type": "string",
                    "example": "_.camelCase(data)"
                }
            }
        },
        "main.jsInvokeRes": {
            "type": "object",
            "required": [
                "result"
            ],
            "properties": {
                "result": {
                    "type": "string",
                    "example": "testData"
                }
            }
        }
    }
}